{
    "multipart": [
        {   "when": { "axis": "x" },
            "apply": { "model": "soviet:block/structural/steel_pylon_horizontal", "uvlock": true }
        },
        {   "when": { "axis": "z"},
            "apply": { "model": "soviet:block/structural/steel_pylon_horizontal", "y": 90, "uvlock": true }
        },
        {   "when": { "axis": "x", "west": "true" },
            "apply": { "model": "soviet:block/structural/steel_pylon_horizontal_west", "uvlock": true }
        },
        {   "when": { "axis": "x", "east": "true" },
            "apply": { "model": "soviet:block/structural/steel_pylon_horizontal_east", "uvlock": true }
        },
        {   "when": { "axis": "x", "north": "true" },
            "apply": { "model": "soviet:block/structural/steel_pylon_horizontal_north", "uvlock": true }
        },
        {   "when": { "axis": "x", "south": "true" },
            "apply": { "model": "soviet:block/structural/steel_pylon_horizontal_south", "uvlock": true }
        },
        {   "when": { "axis": "x", "up": "true" },
            "apply": { "model": "soviet:block/structural/steel_pylon_horizontal_up", "uvlock": true }
        },
        {   "when": { "axis": "x", "down": "true" },
            "apply": { "model": "soviet:block/structural/steel_pylon_horizontal_down", "uvlock": true }
        }
    ,
        {   "when": { "axis": "z", "west": "true" },
            "apply": { "model": "soviet:block/structural/steel_pylon_horizontal_south", "y": 90, "uvlock": true }
        },
        {   "when": { "axis": "z", "east": "true" },
            "apply": { "model": "soviet:block/structural/steel_pylon_horizontal_north", "y": 90, "uvlock": true }
        },
        {   "when": { "axis": "z", "north": "true" },
            "apply": { "model": "soviet:block/structural/steel_pylon_horizontal_west", "y": 90, "uvlock": true }
        },
        {   "when": { "axis": "z", "south": "true" },
            "apply": { "model": "soviet:block/structural/steel_pylon_horizontal_east", "y": 90, "uvlock": true }
        },
        {   "when": { "axis": "z", "up": "true" },
            "apply": { "model": "soviet:block/structural/steel_pylon_horizontal_up", "y": 90, "uvlock": true }
        },
        {   "when": { "axis": "z", "down": "true" },
            "apply": { "model": "soviet:block/structural/steel_pylon_horizontal_down", "y": 90, "uvlock": true }
        }

    ]
}
